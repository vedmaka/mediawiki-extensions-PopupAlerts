{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"env": {
		"browser": true
	},
	"globals": {
		"mediaWiki": false,
		"jQuery": false,
		"ve": false
	},
	"rules": {
		"jsdoc/no-undefined-types": "off",
		"jsdoc/require-param-type": "off",
		"max-len": "off",
		"no-jquery/no-global-selector": "off",
		"no-jquery/no-parse-html-literal": "off",
		"no-new": "off",
		"no-undef": "off"
	}
}
